package switchcase;

import java.util.Scanner;

public class Case {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner in = new Scanner(System.in);//defined object input from scanner class
		
		System.out.println("Enter a number");
		
		String us = in.next();
		
		//System.out.print(us);
		
		switch(us){
			
		default: System.out.println("Outer number");
		break;
		case "Tue": System.out.println("1");
		break;
		case "Wed": System.out.println("2");
		break;	
			
		}

	}

}
