package arra;

import java.util.Arrays;

public class Arry {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[] ab = new int[4];
		
		for(int i = 4 ; i<2 ; i--){
			
			ab[i] = i ;
		}
		//ab[0] = 1;
		//ab[1] = 3;
		//ab[3] = 5;
		
		System.out.println(Arrays.toString(ab));
		
		int[] xy = {1,5,7};
		
		System.out.print(Arrays.toString(xy));
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
